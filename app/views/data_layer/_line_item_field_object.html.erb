{
  "id": "<%= j line_item_field_object.product.id.to_s %>",
  "sku": "<%= j line_item_field_object.product.sku %>",
  "name": "<%= j line_item_field_object.product.name %>",
  "brand": "",
  "category": "<%= j line_item_field_object.product.category %>",
  "variant": "<%= j line_item_field_object.variant.try(:sku) %>",
  "price": <%= line_item_field_object.price.to_f %>,
  "quantity": <%= line_item_field_object.quantity %>,
  "coupon": "",
  "url": "<%= j product_url(line_item_field_object.product) %>",
  "image": "<%= j [request.protocol,request.host,line_item_field_object.product.image_url].join %>",
  "available": <%= line_item_field_object.product.available?.to_json %>
}
